		"clipboardWrite",
		"alarms",
		"storage"
	],

	"requirements": {
		"3D": {
			"features": ["webgl"]
		}
	},
	"kiosk_enabled": true,
	"import": [{"id": "mfaihdlpglflfgpfjcifdjdjcckigekc"}],
