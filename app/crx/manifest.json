		"apkList": [
			"Minidoro.apk"
		],
		"enableExternalDirectory": false,
		"formFactor": "phone",
		"name": "Minidoro",
		"orientation": "landscape",
		"packageName": "com.github.ympavlov.minidoro",
		"resize": "reconfigure"
	}
}