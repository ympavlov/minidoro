{
	"name": "__MSG_extName__",
	"default_locale": "en",
	"manifest_version": 2,
	"version": "10",
	"version_name": "1.0",
	"offline_enabled": true,
	"app": {
		"background": {
			"page": "app_main.html"
		}
	},
	"icons": {
		"16": "minidoro.png",
		"128": "minidoro.png"
	},
	"permissions": [
		"audio",
		"notifications",
		"clipboardRead",
		"accessibilityFeatures.read",
		"unlimitedStorage",
